<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pop_platm" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <TextView
            style="@style/teu_title_style"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="center"
            android:text="@string/_teu_title_text"
            android:textSize="@dimen/sp_20" />

        <LinearLayout
            style="@style/teu_layout_style"
            android:orientation="horizontal" >

            <TextView
                style="@style/teu_tv_style"
                android:layout_weight="5"
                android:gravity="center"
                android:text="@string/_teu_diff_tex" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="3" >

                <TextView
                    android:id="@+id/iv_diffcuty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:focusable="false"
                    android:text="@string/_xinhua"
                    android:textColor="#ff0000"
                    android:textSize="@dimen/sp_18" />

                <EditText
                    android:id="@+id/diffcuty_teu"
                    style="@style/teu_et_style"
                    android:layout_toRightOf="@id/iv_diffcuty" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/teu_layout_style"
            android:orientation="horizontal" >

            <TextView
                style="@style/teu_tv_style"
                android:layout_weight="5"
                android:gravity="center"
                android:text="@string/_btc_price_text" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="3" >

                <TextView
                    android:id="@+id/iv_china_teu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:focusable="false"
                    android:text="@string/_xinhua"
                    android:textColor="#ff0000"
                    android:textSize="@dimen/sp_18" />

                <EditText
                    android:id="@+id/china_teu"
                    style="@style/teu_et_style"
                    android:layout_toRightOf="@id/iv_china_teu" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/teu_layout_style"
            android:orientation="horizontal" >

            <TextView
                style="@style/teu_tv_style"
                android:layout_weight="5"
                android:gravity="center"
                android:text="@string/_gh_s_text" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="3" >

                <TextView
                    android:id="@+id/iv_canteu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:focusable="false"
                    android:text="@string/_xinhua"
                    android:textColor="#ff0000"
                    android:textSize="@dimen/sp_18" />

                <EditText
                    android:id="@+id/can_teu"
                    style="@style/teu_et_style"
                    android:layout_toRightOf="@id/iv_canteu" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/teu_layout_style"
            android:orientation="horizontal" >

            <TextView
                style="@style/teu_tv_style"
                android:layout_weight="5"
                android:gravity="center"
                android:text="@string/_dev_cost_text" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="3" >

                <TextView
                    android:id="@+id/iv_dev_cost"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:focusable="false"
                    android:text="@string/_xinhua"
                    android:textColor="#ff0000"
                    android:textSize="@dimen/sp_18" />

                <EditText
                    android:id="@+id/dev_cost"
                    style="@style/teu_et_style"
                    android:layout_toRightOf="@id/iv_dev_cost" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/teu_layout_style"
            android:orientation="horizontal" >

            <TextView
                style="@style/teu_tv_style"
                android:layout_weight="5"
                android:gravity="center"
                android:text="@string/_free_text" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="3" >

                <TextView
                    android:id="@+id/iv_mining_poundage_teu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:focusable="false"
                    android:text="@string/_xinhua"
                    android:textColor="#ff0000"
                    android:textSize="@dimen/sp_18" />

                <EditText
                    android:id="@+id/mining_poundage_teu"
                    style="@style/teu_et_style"
                    android:layout_toRightOf="@id/iv_mining_poundage_teu" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/teu_layout_style"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="horizontal" >

            <TextView
                style="@style/teu_tv_style"
                android:layout_weight="5"
                android:gravity="center"
                android:text="@string/_start_date_text" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="3" >

                <TextView
                    android:id="@+id/iv_begin_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:focusable="false"
                    android:text="@string/_xinhua"
                    android:textColor="#ff0000"
                    android:textSize="@dimen/sp_18" />

                <EditText
                    android:id="@+id/begin_date"
                    style="@style/teu_et_style"
                    android:layout_toRightOf="@id/iv_begin_date" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/teu_layout_style"
            android:orientation="horizontal" >

            <TextView
                style="@style/teu_tv_style"
                android:layout_weight="5"
                android:gravity="center"
                android:text="@string/_w_text" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="3" >

                <TextView
                    android:id="@+id/iv_power_teu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:focusable="false"
                    android:text="@string/_xinhua"
                    android:textColor="#ff0000"
                    android:textSize="@dimen/sp_18" />

                <EditText
                    android:id="@+id/power_teu"
                    style="@style/teu_et_style"
                    android:layout_toRightOf="@id/iv_power_teu" />
            </RelativeLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <RelativeLayout
                android:id="@+id/hidden1_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible" >

                <Button
                    android:id="@+id/btn_hidden1_ll"
                    android:layout_width="@dimen/dp_80"
                    android:layout_height="@dimen/dp_30"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dp_10"
                    android:background="@drawable/btn_simple_selector"
                    android:text="@string/_look_more"
                    android:textColor="@color/argb_ffffff" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/hidden_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone" >

                <LinearLayout
                    style="@style/teu_layout_style"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/teu_tv_style"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text="@string/_diff_add_text" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3" >

                        <TextView
                            android:id="@+id/iv_diffcuty_add_teu"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:focusable="false"
                            android:text="@string/_xinhua"
                            android:textColor="#ff0000"
                            android:textSize="@dimen/sp_18"
                            android:visibility="invisible" />

                        <EditText
                            android:id="@+id/diffcuty_add_teu"
                            style="@style/teu_et_style"
                            android:layout_toRightOf="@id/iv_diffcuty_add_teu"
                            android:text="@string/_20_text" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    style="@style/teu_layout_style"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/teu_tv_style"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text="@string/_add_date_text" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3" >

                        <TextView
                            android:id="@+id/iv_diffcuty_add_date_teu"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:focusable="false"
                            android:text="@string/_xinhua"
                            android:textColor="#ff0000"
                            android:textSize="@dimen/sp_18"
                            android:visibility="invisible" />

                        <EditText
                            android:id="@+id/diffcuty_add_date_teu"
                            style="@style/teu_et_style"
                            android:layout_toRightOf="@id/iv_diffcuty_add_date_teu"
                            android:text="@string/_11_text" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    style="@style/teu_layout_style"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/teu_tv_style"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text="@string/_kwh_text" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3" >

                        <TextView
                            android:id="@+id/iv_power_cost_teu"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:focusable="false"
                            android:text="@string/_xinhua"
                            android:textColor="#ff0000"
                            android:textSize="@dimen/sp_18"
                            android:visibility="invisible" />

                        <EditText
                            android:id="@+id/power_cost_teu"
                            style="@style/teu_et_style"
                            android:layout_toRightOf="@id/iv_power_cost_teu"
                            android:text="@string/_0_6_text" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    style="@style/teu_layout_style"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/teu_tv_style"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text="@string/_other_cost" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3" >

                        <TextView
                            android:id="@+id/iv_dev_others_cost"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:focusable="false"
                            android:text="@string/_xinhua"
                            android:textColor="#ff0000"
                            android:textSize="@dimen/sp_18"
                            android:visibility="invisible" />

                        <EditText
                            android:id="@+id/dev_others_cost"
                            style="@style/teu_et_style"
                            android:layout_toRightOf="@id/iv_dev_others_cost"
                            android:text="@string/_100_text" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    style="@style/teu_layout_style"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/teu_tv_style"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text="@string/_dev_startdate" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3" >

                        <TextView
                            android:id="@+id/iv_dev_startdate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:focusable="false"
                            android:text="@string/_xinhua"
                            android:textColor="#ff0000"
                            android:textSize="@dimen/sp_18"
                            android:visibility="invisible" />

                        <EditText
                            android:id="@+id/dev_startdate"
                            style="@style/teu_et_style"
                            android:layout_toRightOf="@id/iv_dev_startdate" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    style="@style/teu_layout_style"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/teu_tv_style"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text="@string/_dev_stopdate" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3" >

                        <TextView
                            android:id="@+id/iv_dev_stopdate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:focusable="false"
                            android:text="@string/_xinhua"
                            android:textColor="#ff0000"
                            android:textSize="@dimen/sp_18"
                            android:visibility="invisible" />

                        <EditText
                            android:id="@+id/dev_stopdate"
                            style="@style/teu_et_style"
                            android:layout_toRightOf="@id/iv_dev_stopdate" />
                    </RelativeLayout>
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/hidden2_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10" >

                    <Button
                        android:id="@+id/btn_hidden2_ll"
                        android:layout_width="@dimen/dp_80"
                        android:layout_height="@dimen/dp_30"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/dp_6"
                        android:background="@drawable/btn_simple_selector"
                        android:text="@string/_text_yincang"
                        android:textColor="@color/argb_ffffff"
                        android:textSize="@dimen/sp_16" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_10"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/hint_tv2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:gravity="center_vertical"
                android:paddingTop="@dimen/dp_6"
                android:text="@string/_hint_text2"
                android:textColor="#ff0000" />

            <TextView
                android:id="@+id/hint_tv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:gravity="center_vertical"
                android:text="@string/_hint_text1"
                android:textColor="#ff0000"
                android:visibility="invisible" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/js_btn_teu"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:background="@drawable/btn_simple_selector"
                android:clickable="false"
                android:text="@string/_js_text"
                android:textColor="@color/argb_ffffff"
                android:textSize="@dimen/sp_20" />
        </LinearLayout>
    </LinearLayout>

</ScrollView>